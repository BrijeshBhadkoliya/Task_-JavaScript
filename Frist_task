<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <script>
        let One =  Number(prompt("Enter First Number:"));
        let Two =  Number(prompt("Enter Second Number:"));
    
       
        let New = prompt("Enter 1 for +, 2 for -, 3 for *, 4 for /, 5 for %");
    
        switch (Number(New)) {
            case 1:
                document.write(`${One} + ${Two} = ${One + Two}`);
                break;
            case 2:
                document.write(`${One} - ${Two} = ${One - Two}`);
                break;
            case 3:
                document.write(`${One} * ${Two} = ${One * Two}`);
                break;
            case 4:
                document.write(`${One} / ${Two} = ${One / Two}`);
                break;
            case 5:
                document.write(`${One} % ${Two} = ${One % Two}`);
                break;
            default:
                document.write("Not Defined This Process");
                break;
        }
    </script>
    
</body>

</html>
